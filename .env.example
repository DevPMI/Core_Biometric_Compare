# Database Configuration
# Ganti dengan URI database PostgreSQL Anda
DATABASE_URL=postgresql://user:password@localhost:5432/dbname

# Flask Settings
# Ganti dengan random string yang aman untuk production
SECRET_KEY=ganti-dengan-secret-key-yang-aman
FLASK_APP=run.py
FLASK_ENV=development

# Biometric Settings
FACE_MODEL=ArcFace
FACE_THRESHOLD=0.68
PALM_MATCH_THRESHOLD=0.15
PALM_ORB_FEATURES=1000

# File Upload Settings
MAX_CONTENT_LENGTH=16777216

# API Security
API_KEY=ganti-dengan-api-key-kustom-yang-panjang-dan-acak
