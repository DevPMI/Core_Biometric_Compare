# Database config
DATABASE_URL=postgresql://postgres:postgres@db:5432/User_Biometrics

# Flask Settings
# Ganti dengan random string yang aman untuk production
SECRET_KEY=ganti-dengan-secret-key-yang-aman
FLASK_APP=run.py
FLASK_ENV=development

# Face Recognition config (Optional)
# FACE_MODEL=VGG-Face
# FACE_THRESHOLD=0.40

# Biometric Settings
FACE_MODEL=ArcFace
FACE_THRESHOLD=0.68
PALM_MATCH_THRESHOLD=0.15
PALM_ORB_FEATURES=1000

# File Upload Settings
MAX_CONTENT_LENGTH=16777216

# API Security
API_KEY=ganti-dengan-api-key-kustom-yang-panjang-dan-acak
